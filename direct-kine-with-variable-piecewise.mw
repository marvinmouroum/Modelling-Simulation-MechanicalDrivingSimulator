<Group view="code" labelreference="L56961" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">draw_slider := proc(P,h,data,color:=&quot;Red&quot;)

local peak := P:

local low_1  := make_POINT(ground,0,comp_Y(P)-2*h/2,comp_Z(P)-h/2):

local low_2  := make_POINT(ground,0,comp_Y(P)-2*h,comp_Z(P)-h/2):

local low_3  := make_POINT(ground,0,comp_Y(P)-2*h,comp_Z(P)-h):

local low_4  := make_POINT(ground,0,comp_Y(P)+2*h,comp_Z(P)-h):

local low_5  := make_POINT(ground,0,comp_Y(P)+2*h,comp_Z(P)-h/2):

local low_6  := make_POINT(ground,0,comp_Y(P)+2*h/2,comp_Z(P)-h/2):

local Mat := LinearAlgebra:-Transpose( &lt;&lt;comp_XYZ(peak,ground)&gt;|&lt;comp_XYZ(low_1,ground)&gt;|&lt;comp_XYZ(low_2,ground)&gt;|&lt;comp_XYZ(low_3,ground)&gt;|&lt;comp_XYZ(low_4,ground)&gt;|&lt;comp_XYZ(low_5,ground)&gt; |&lt;comp_XYZ(low_6,ground)&gt; &gt; ):

local platform_profile := subs(data,Mat):
local np := RowDimension(platform_profile):

local l := [seq([platform_profile[i,2],platform_profile[i,3]],i=1..np)]:

global body := draw_body_2D(ground,data,{}, l,body_col=color,opts=[transparency=0.5],reference_frame=false);

end proc:</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L56951" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">draw_inc := proc(P,h,angle,data,color:=&quot;Red&quot;)

local p := make_POINT(ground,0,comp_Y(P)+cos(angle)*h,comp_Z(P)+sin(angle)*h):

global line := draw_line_2D(P,p,data,line_col=color,line_width=5);

end proc:</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L56950" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">draw_platform := proc(x,y,z,theta,L,H,L__s,data:={},color:=&quot;Gray&quot;)

<Font foreground="[0,0,0]">#should result from kinematic model
local </Font>RF__platform := subs(data,translate(x,y,z).rotate('X',theta));


<Font foreground="[0,0,0]">#corner points of the shape
</Font>local P__1 := make_POINT(RF__platform,0,-L/2,+H/2):
local P__2 := make_POINT(RF__platform,0,+L/2,+H/2):
local P__3 := make_POINT(RF__platform,0,+L/2,-H/2):
local P__4 := make_POINT(RF__platform,0,+L/2 - (L-L__s)/2,-H):
local P__5 := make_POINT(RF__platform,0,-L/2 + (L-L__s)/2,-H):
local P__6 := make_POINT(RF__platform,0,-L/2,-H/2):


<Font foreground="[0,0,0]">#create a matrix of the points</Font>
local Mat := LinearAlgebra:-Transpose( &lt;&lt;comp_XYZ(P__1,ground)&gt;|&lt;comp_XYZ(P__2,ground)&gt;|&lt;comp_XYZ(P__3,ground)&gt;|&lt;comp_XYZ(P__4,ground)&gt;|&lt;comp_XYZ(P__5,ground)&gt;|&lt;comp_XYZ(P__6,ground)&gt;&gt; ):


<Font foreground="[0,0,0]">#insert the data
</Font>local platform_profile := subs(data,%):
local np := RowDimension(platform_profile):

local l := [seq([platform_profile[i,2],platform_profile[i,3]],i=1..np)]:

global body := draw_body_2D(ground,data,{}, l,body_col=color,opts=[transparency=0.5],reference_frame=false);

end proc:</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L56949" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">draw_mech := proc(data)


local sol := op(subs(data, {s}));
local mbs:
  mbs := [

          draw_platform(0,Y,Z,-theta__2,d+X,20,d+X-(d+X)/4,data),
          draw_frame_2D(RF__M,{op(data),sol},line_col = &quot;Black&quot;,line_scale=25),
          
          draw_frame_2D(RF__11,{op(data),sol},line_col = &quot;Red&quot;,line_scale=10),
          draw_inc(project(origin(RF__11),ground),r,theta__1(t),{op(data),sol},&quot;Red&quot;),
          draw_frame_2D(RF__12,[op(data),sol],line_col = &quot;Pink&quot;,line_scale=10),
          draw_inc(project(origin(RF__12),ground),100,Pi-alpha,{op(data),sol},&quot;Pink&quot;),
          draw_slider(project(origin(RF__11),ground),20,{op(data),sol},&quot;Red&quot;),
          draw_point_2D(P1,[op(data)],point_size=1,point_col=&quot;Black&quot;),
          
          draw_point_2D(P3,[op(data),sol],point_size=1,point_col=&quot;Black&quot;),
          draw_frame_2D(RF__31,{op(data),sol},line_col = &quot;Blue&quot;,line_scale=10),
          draw_slider(project(origin(RF__31),ground),20,{op(data),sol},&quot;Blue&quot;),
          draw_inc(project(origin(RF__31),ground),r,Pi-theta__3(t),{op(data),sol},&quot;Blue&quot;),
          draw_frame_2D(RF__32,[op(data),sol],line_col = &quot;SkyBlue&quot;,line_scale=10),
          draw_inc(project(origin(RF__32),ground),100,alpha,{op(data),sol},&quot;SkyBlue&quot;)

          ]:
  

  draw_mbs_2D(mbs):
end proc:</Text-field>
</Input>
</Group></Presentation-Block>
