<Group view="presentation" inline-output="false" labelreference="L56984" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field style="Text" layout="Normal">Tool to allow us to calculate the velocity and displacement and acceleration based on a piecewise function</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L56986" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">get_vel_disp := proc(acc2,a1,ti1,Tf1,T,v__0,x__0)

local acc := subs(a=a1,ti=ti1,Tf=Tf1,acc2);

local ac := eval(acc,t=T):
local v := eval(acc,t=T)*T+v__0:
local d := eval(acc,t=T)*(T^2/2)+v__0(t)+x__0:

global result :=&lt;ac,v,d&gt;:

end proc:</Text-field>
</Input>
</Group>
