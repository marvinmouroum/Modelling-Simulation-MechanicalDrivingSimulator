local low_4  := make_POINT(ground,0,comp_Y(P)+2*h,comp_Z(P)-h):

local low_5  := make_POINT(ground,0,comp_Y(P)+2*h,comp_Z(P)-h/2):

local low_6  := make_POINT(ground,0,comp_Y(P)+2*h/2,comp_Z(P)-h/2):

local Mat := LinearAlgebra:-Transpose( &lt;&lt;comp_XYZ(peak,ground)&gt;|&lt;comp_XYZ(low_1,ground)&gt;|&lt;comp_XYZ(low_2,ground)&gt;|&lt;comp_XYZ(low_3,ground)&gt;|&lt;comp_XYZ(low_4,ground)&gt;|&lt;comp_XYZ(low_5,ground)&gt; |&lt;comp_XYZ(low_6,ground)&gt; &gt; ):

local platform_profile := subs(data,Mat):
local np := RowDimension(platform_profile):

local l := [seq([platform_profile[i,2],platform_profile[i,3]],i=1..np)]:

global body := draw_body_2D(ground,data,{}, l,body_col=color,opts=[transparency=0.5],reference_frame=false);

end proc:</Text-field>
