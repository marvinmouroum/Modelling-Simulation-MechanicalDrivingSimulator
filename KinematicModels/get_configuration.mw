<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">get_configuration := proc( q__3, y, z, theta )
    description &quot;read the configuration of the system using an end effector position&quot;;

local d := (q) -&gt; 10;

local EF := Translate(0,y,z).Rotate('X',theta);
local psi := Pi/3;

local r := 10;

local R:=r:local P1 := EF.translate(0,-d(q__3)/2,0).&lt;0,0,0,1&gt;:
local P3 := EF.translate(0,d(q__3)/2,0).&lt;0,0,0,1&gt;:local RF__11 := Translate(0,q__11,0):
local RF__12 := Translate(0,q__12,0).Rotate('Z',Pi):

local RF__31 := Translate(0,q__31,0).Rotate('Z',Pi):
local RF__32 := Translate(0,q__32,0):local P__1_11 := RF__11.Rotate('X',alpha__1).Translate(0,R,0).&lt;0,0,0,1&gt;:
local P__1_12 := RF__12.Rotate('X',psi).Translate(0,l__1,0).&lt;0,0,0,1&gt;:

local P__3_31 := RF__31.Rotate('X',alpha__3).Translate(0,R,0).&lt;0,0,0,1&gt;:
local P__3_32 := RF__32.Rotate('X',psi).Translate(0,l__3,0).&lt;0,0,0,1&gt;:# the points need to be the same
local c_11 := P__1_11[1] = P1[1]:
local c_12 := P__1_11[2] = P1[2]:
local c_13 := P__1_11[3] = P1[3]:

local c_21 := P__1_12[1] = P1[1]:
local c_22 := P__1_12[2] = P1[2]:
local c_23 := P__1_12[3] = P1[3]:

local c_31 := P__3_31[1] = P3[1]:
local c_32 := P__3_31[2] = P3[2]:
local c_33 := P__3_31[3] = P3[3]:

local c_41 := P__3_32[1] = P3[1]:
local c_42 := P__3_32[2] = P3[2]:
local c_43 := P__3_32[3] = P3[3]:

local c_51 := P__1_11[1] = P__1_12[1]:
local c_52 := P__1_11[2] = P__1_12[2]:
local c_53 := P__1_11[3] = P__1_12[3]:

local c_61 := P__3_32[1] = P__3_31[1]:
local c_62 := P__3_32[2] = P__3_31[2]:
local c_63 := P__3_32[3] = P__3_31[3]:local alpha1 := solve({c_13},alpha__1):
#evalf(alpha1):
local sol := subs(alpha1,c_12):
global q__11f := solve(sol,q__11):

local l1 := solve({c_23},l__1):
#evalf(evalf(l1)):
local sol2 := subs(l1,c_22):
global q__12f := solve(sol2,q__12):

local alpha3 := solve({c_33},alpha__3):
#evalf(alpha3):
local sol3 := subs(alpha3,c_32):
global q__31f := solve(sol3,q__31):

local l3 := solve({c_43},l__3):
#evalf(evalf(l3)):
local sol4 := subs(l3,c_42):
global q__32f := solve(sol4,q__32):local A11 := simplify(subs(alpha1,q__11=q__11f,subs(R=r,P__1_11))):
